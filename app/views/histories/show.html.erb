<div class="page-header">
  <h3>History #<%= @history.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Pseudoid</dt>
      <dd><%= @history.pseudoID %></dd>

      <dt>Course data</dt>
      <dd><%= @history.course_data %></dd>

      <dt>Course num</dt>
      <dd><%= @history.course_num %></dd>

      <dt>Year</dt>
      <dd><%= @history.year %></dd>

      <dt>Quarter num</dt>
      <dd><%= @history.quarter_num %></dd>

      <dt>Quarter text</dt>
      <dd><%= @history.quarter_text %></dd>

      <dt>Section</dt>
      <dd><%= @history.section %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/histories" class="btn btn-primary">
        Back
      </a>
      <a href="/histories/<%= @history.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_history/<%= @history.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
